        "codelyzer": "^5.2.2",
        "del": "^5.1.0",
        "gulp": "^4.0.2",
        "gulp-clean-css": "^4.3.0",
        "gulp-concat": "^2.6.1",
        "gulp-concat-css": "^3.1.0",
        "gulp-connect": "^5.6.1",
        "gulp-debug": "^4.0.0",
