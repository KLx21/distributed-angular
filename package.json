{
    "name": "tci",
    "version": "2.0.0",
    "description": "TIBCO Cloud Integration",
    "private": true,
    "bin": "./server/main.js",
    "keywords": [],
    "author": "Tibco Software Inc.",
    "license": "SEE LICENSE IN license.txt",
    "scripts": {
        "xpreinstall": "npm i mozilla/npm-lockdown#v0.0.8 && npm run lockdown",
        "lockdown": "./node_modules/.bin/lockdown",
        "lockdown-relock": "./node_modules/.bin/lockdown-relock",
        "start": "node --harmony server/main.js 2>&1",
        "start-express": "node --harmony server/main.js 2>&1",
        "start-express-inspect": "node --inspect-brk=0.0.0.0:9229 server/main.js 2>&1",
        "inspect": "WS_RUN_MODE=${WS_RUN_MODE:-\"test\"} NODE_ENV=production node --inspect=0.0.0.0:9229 server/main.js",
        "inspect-brk": "WS_RUN_MODE=${WS_RUN_MODE:-\"prod\"} NODE_ENV=production node --inspect-brk=0.0.0.0:9229 dist/server/app.js",
        "build-linux": "pkg -t node10-alpine-x64 -o ./app.linux . | grep -v \"\\[debug\\]\"",
        "prebuild-linux-src": "source ../nodeapks.sh  node::apk::install",
        "postbuild-linux-src": "source ../nodeapks.sh  node::apk::uninstall",
        "build-linux-src": "pkg -t node10-alpine-x64 -b -o ./app.linux .",
        "build-mac": "pkg  -t latest-mac-x64 -o ./app.mac . | grep -v \"\\[debug\\]\"",
        "test-scribe-inspect": "WS_RUN_MODE=${WS_RUN_MODE:-\"test\"} mocha --exit --no-colors --inspect-brk=0.0.0.0:9229 --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,xunit=unit-test-results/test-results.xml --timeout 12000000 $(find dist/server/test -name scribe.spec.js)",
        "test-tci-inspect": "WS_RUN_MODE=${WS_RUN_MODE:-\"test\"} mocha --exit --no-colors --inspect-brk=0.0.0.0:9229 --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,xunit=unit-test-results/test-results.xml --timeout 12000000 $(find dist/server/test -name tci.spec.js)",
        "test-app-inspect": "WS_RUN_MODE=${WS_RUN_MODE:-\"test\"} mocha --exit --no-colors --inspect=0.0.0.0:9229 --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,xunit=unit-test-results/test-results.xml --timeout 12000000 $(find dist/server/test -name app.spec.js)",
        "test-64-inspect": "WS_RUN_MODE=${WS_RUN_MODE:-\"test\"} mocha --exit --no-colors --inspect=0.0.0.0:9229 --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,xunit=unit-test-results/test-results.xml --timeout 12000000 $(find dist/server/test -name base64.spec.js)",
        "test-chash-inspect": "WS_RUN_MODE=${WS_RUN_MODE:-\"test\"} mocha --exit --no-colors --inspect=0.0.0.0:9229 --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,xunit=unit-test-results/test-results.xml --timeout 12000000 $(find dist/server/test -name chash.spec.js)",
        "test-user-inspect": "WS_RUN_MODE=${WS_RUN_MODE:-\"test\"} mocha --exit --no-colors --inspect-brk=0.0.0.0:9229 --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,xunit=unit-test-results/test-results.xml --timeout 12000000 $(find dist/server/test -name testuser.spec.js)",
        "test-version-inspect": "WS_RUN_MODE=${WS_RUN_MODE:-\"test\"} mocha --exit --no-colors --inspect-brk=0.0.0.0:9229 --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,xunit=unit-test-results/test-results.xml --timeout 12000000 $(find dist/server/test -name version.spec.js)",
        "test-schema": "WS_RUN_MODE=${WS_RUN_MODE:-\"test\"} mocha --exit --no-colors --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,xunit=unit-test-results/test-results.xml --timeout 12000000 $(find dist/server/test -name jsonschema.spec.js)",
        "test-server": "WS_RUN_MODE=${WS_RUN_MODE:-\"test\"} mocha --exit --no-colors --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,xunit=unit-test-results/test-results.xml --timeout 12000000 $(find dist/server/test -name *.spec.js | sort)",
        "test-server-inspect": "WS_RUN_MODE=${WS_RUN_MODE:-\"test\"} mocha --exit --inspect=0.0.0.0:9229  --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,xunit=unit-test-results/test-results.xml --timeout 12000000 $(find dist/server/test -name *.spec.js)",
        "test-server-inspect-brk": "WS_RUN_MODE=${WS_RUN_MODE:-\"test\"} mocha --exit --inspect-brk=0.0.0.0:9229  --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,xunit=unit-test-results/test-results.xml --timeout 12000000 $(find dist/server/test -name *.spec.js)",
        "test-awslocal": "WS_RUN_MODE=${WS_RUN_MODE:-\"test\"} mocha --exit --no-colors --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,xunit=unit-test-results/awslocal-results.xml --timeout 12000000 $(find dist/server/test -name *.awsspec.js)",
        "test-awslocal-inspect": "WS_RUN_MODE=${WS_RUN_MODE:-\"test\"} mocha --exit --inspect=0.0.0.0:9229  --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,xunit=unit-test-results/awslocal-results.xml --timeout 12000000 $(find dist/server/test -name *.awsspec.js)",
        "test-awslocal-inspect-brk": "WS_RUN_MODE=${WS_RUN_MODE:-\"test\"} mocha --exit --inspect-brk=0.0.0.0:9229  --compilers js:babel-core/register --reporter mocha-multi --reporter-options spec=-,xunit=unit-test-results/awslocal-results.xml --timeout 12000000 $(find dist/server/test -name *.awsspec.js)",
        "test": "karma start karma.conf.js --browsers Chrome_custom --reporters teamcity,coverage,junit,kjhtml",
        "test-once": "karma start karma.conf.js --single-run --browsers Chrome_custom --reporters teamcity,coverage,junit",
        "webdriver:update": "webdriver-manager update --standalone false --gecko false",
        "pretest:once": "yarn run build:test",
        "test:once": "karma start karma.conf.js --single-run --browsers Chrome_custom",
        "build:test": "tsc -p src/",
        "preprotractor": "protractorstart",
        "protractor": "protractor protractor.config.js",
        "preserve:e2e": "yarn run build:test",
        "serve:e2e": "WISTUDIO_RUN_MODE=${WISTUDIO_RUN_MODE:-\"prod\"} NODE_ENV=production node dist/server/main.js",
        "e2e": "concurrently \"yarn run serve:e2e\" \"yarn run protractor\" --kill-others --success first",
        "poste2e": "protractorstop",
        "tsc:w": " -w",
        "i18n": "ng-xi18n",
        "rollup": "rollup -c rollup.config.js",
        "build": "gulp --env ${MODE:-\"development\"} --env ${YARN_REGISTRY:-\"https://registry.yarnpkg.com\"} --gulpfile gulpfile.babel.js build-dist",
        "build-api": "gulp --env ${MODE:-\"development\"} --env ${YARN_REGISTRY:-\"https://registry.yarnpkg.com\"} --gulpfile gulpfile.babel.js build-api",
        "buildt": "gulp --tindex --env ${MODE:-\"development\"} --env ${YARN_REGISTRY:-\"https://registry.yarnpkg.com\"} --gulpfile gulpfile.babel.js build-dist",
        "build-server": "gulp --env ${MODE:-\"development\"} --env ${YARN_REGISTRY:-\"https://registry.yarnpkg.com\"} --gulpfile gulpfile.babel.js build-server",
        "webpack": "webpack --config ./webpack.config.js --progress"
    },
    "pkg": {
        "assets": [
            "client/**/*",
            "server/**/*"
        ]
    },
    "dependencies": {
        "@angular/common": "9.1.11",
        "@angular/compiler": "9.1.11",
        "@angular/core": "9.1.11",
        "@angular/forms": "^9.1.11",
        "@angular/platform-browser": "^9.1.11",
        "@angular/platform-browser-dynamic": "^9.1.11",
