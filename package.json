        "gulp-tslint": "^8.1.3",
        "gulp-typescript": "^5.0.1",
        "html-minifier": "^4.0.0",
        "jasmine-core": "^3.5.0",
        "karma": "^5.1.0",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage": "^2.0.2",
